{
    "name": "hinchk/zorkery-cli",
    "description": "Zork-Style Guided Stories using Laravel Zero.",
    "keywords": ["framework", "laravel", "laravel zero", "console", "cli"],
    "homepage": "https://imustnotfear.net",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/hinchk/zorkery-cli/issues",
        "source": "https://github.com/hinchk/zorkery-cli"
    },
    "authors": [
        {
            "name": "Kasey Hinchman",
            "email": "kasey.hinchman@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/log": "^8.0",
        "laminas/laminas-text": "^2.8",
        "laravel-zero/framework": "^8.7",
        "nunomaduro/laravel-console-menu": "^3.2",
        "padraic/phar-updater": "^1.0.6"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "pestphp/pest": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["zorkery-cli"]
}
